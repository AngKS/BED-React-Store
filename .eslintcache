/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/index.js",[],["170","171"],"/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/App.js",[],"/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/components/navbar/Navbar.jsx",["172","173"],"\n\nimport React, { useState, useEffect } from 'react'\nimport { Link } from 'react-router-dom'\n\nimport './Navbar.css'\n\n\nfunction Navbar() {\n\n    const [click, setClick] = useState(false)\n    const [button, setButton] = useState(true)\n    const [user, setUser] = useState()\n\n\n    const handleClick = () => setClick(!click)\n    const closeMobileMenu = () => setClick(false)\n    const showBtn = () => {\n        if (window.innerWidth <= 960) {\n            setButton(false)\n        }\n        else {\n            setButton(true)\n        }\n    }\n    const getUser = () => {\n        let USER = localStorage.getItem('user')\n        USER = USER ? JSON.parse(USER)[0] : null\n        setUser(USER)\n    }\n    useEffect(() => {\n        showBtn()\n        getUser()\n        \n    }, [])\n\n    window.addEventListener('resize', showBtn)\n    return (\n        <>\n            <nav className=\"navbar\">\n                <div className=\"navbar-container\">\n                    <Link to=\"/\" className=\"navbar-logo\" onClick={closeMobileMenu}>\n                        <h2>Game<i className=\"text-danger\">SP</i>ot</h2>\n                    </Link>\n                    <div className=\"menu-icon\" onClick={handleClick}>\n                        <i className={click ? 'fas fa-times' : 'fas fa-bars'} />\n                    </div>\n                    <ul className={click ? 'nav-menu active' : 'nav-menu'}>\n                        \n                        <li className='nav-item'>\n                            <Link to='/store' className='nav-links' onClick={closeMobileMenu}>\n                                Store\n                            </Link>\n                        </li>\n                        <li className='nav-item'>\n                            <Link to='/multipage' className='nav-links' onClick={closeMobileMenu}>\n                                {\n                                    user ? (\n                                        (user.type == 'admin') ? \"admin\" : \"user\"\n                                    ) : \"Community\"\n                                }\n                            </Link>\n                        </li>\n                        <li className='nav-item'>\n                            {user ? (\n                                <Link to='/login' className='nav-links' onClick={closeMobileMenu}>\n                                    <i className=\"fas fa-user\" /><span className=\"my-0 p-2\">{user.username}</span>\n                                </Link>\n                            ) : (\n                                <Link to='/login' className='nav-links' onClick={closeMobileMenu}>\n                                    <i className=\"fas fa-user\" />\n                                </Link>\n                                )\n                            }\n                        </li>\n                        <li className='nav-item'>\n                            <Link to='/cart' className='nav-links' onClick={closeMobileMenu}>\n                                <i className=\"fas fa-shopping-cart\" /><span className=\"my-0 p-2\">Cart</span>\n                            </Link>\n                        </li>\n                    </ul>\n                </div>\n            </nav>\n        </>\n    )\n}\n\nexport default Navbar\n","/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/components/pages/Home.jsx",[],"/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/components/searchBar/Search.jsx",["174","175","176","177","178"],"/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/components/games/Games.jsx",[],"/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/components/games/gameCard/gameCard.jsx",[],"/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/components/pages/Store.jsx",["179"],"import React, { useEffect } from 'react'\nimport Games from '../games/Games'\nimport Navbar from '../navbar/Navbar'\nimport Search from '../searchBar/Search'\n\nfunction Store() {\n\n    useEffect(() => {\n        // localStorage.clear()\n    }, [])\n\n    return (\n        <div>\n            <Navbar />\n\n            <div className=\"container\">\n                <h1>Games Catalogue</h1>\n                <hr />\n                <Search />\n            </div>\n\n        </div>\n    )\n}\n\nexport default Store\n","/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/components/heroSection/HeroSection.js",[],"/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/components/homeMain/Main.jsx",["180","181","182"],"import React, { useEffect, useState } from 'react'\n\nimport \"./main.css\"\nimport Games from '../games/Games'\nimport Search from '../searchBar/Search'\n\n\nfunction Main() {\n\n\n    return (\n        <div className=\"main-container mx-auto text-center\">\n            <section className=\"banner\">\n                <h1><strong>Welcome to SP GameSPot</strong></h1>\n\n            </section>  \n            {/* <section className=\"homeSearch mx-auto\"><Search /></section> */}\n            <Games />\n        </div>\n    )\n}\n\nexport default Main\n","/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/components/product/Product.jsx",["183","184","185","186"],"/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/components/cart/Cart.jsx",[],"/Users/angks/Desktop/School/Year_1/Sem_02/ST0503_Back-End_Web_Development/Assignment02/V2/Project/client/src/components/reviews/Reviews.jsx",["187"],"import React, { useEffect, useState } from 'react'\nimport {Link} from 'react-router-dom'\nimport axios from 'axios'\nimport \"./reviews.css\"\n\n\nfunction Reviews() {\n\n    const [reviews, setReview] = useState()\n    const [user, setUser] = useState()\n\n    const [ratings, setRatings] = useState()\n    const [userReview, setUserReview] = useState()\n    const [submitted, setSubmit] = useState(false)\n    \n    useEffect(() => {\n        let gameKey = localStorage.getItem(\"game\")\n        axios.get(`http://localhost:8081/reviews/${gameKey}`).then(res => {\n            const allReviews = res.data\n            setReview(allReviews)\n            \n        })\n        let userID = localStorage.getItem(\"user\")\n        userID = userID ? JSON.parse(userID)[0] : null\n        setUser(userID)\n    }, [])\n\n    let sendReview = () => {\n        let header = { 'authorization': 'Bearer ' + localStorage.getItem('token') }\n        axios({\n            method : \"POST\",\n            headers: header,\n            url : \"http://localhost:8081/reviews\",\n            data : {\n                userID : user.userid,\n                gameID : localStorage.getItem(\"game\"),\n                content : userReview,\n                rating : (ratings ? ratings : \"0\"),\n            }\n        }).then(res =>{\n            if (res.data.auth == false){\n                return <div className=\"\">Authorization Failed</div>\n            }\n            setSubmit(check => !check)\n        })\n        \n        \n    }\n\n    return (\n        <div>\n            <section className=\"reviews-container\">\n                <h3 className=\"border-top border-bottom py-3\">Reviews</h3>\n                {\n\n                    reviews ? (\n                        (reviews.length > 0) ? (\n                            reviews.map(review => {\n                                return (\n                                    <div className=\"media review rounded\">\n                                        <div className=\"media-body\">\n                                            <div>\n                                                {[...Array(review.rating)].map(star => {\n                                                    return (<i className=\"fas fa-star text-warning\" />)\n                                                })}\n                                            </div>\n                                            <div className=\"ratingUser d-flex\">\n                                                <p className=\"metadata mr-auto\"><small>by {review.username}</small></p>\n                                                <p className=\"metadata\"><small><i>~ Posted on {(review.created_at.slice(0, 10))}</i></small></p>\n                                            </div>\n\n                                            <p>{review.content}</p>\n                                        </div>\n                                        <hr />\n                                    </div>\n\n                                )\n                            })\n                        ) : (\n                            <h5 className=\"text-secondary text-center my-5\">No reviews found</h5>\n                            \n                        )\n                        \n\n                        )\n                    :\n\n                        (\n\n                            <div className=\"\">\n                                \n                                <h3>Loading...</h3>\n                                <div class=\"spinner-border\"></div>\n                            </div>\n\n\n                        )\n\n                    \n\n                }\n            </section>\n            <section className=\"newReview\">\n                <form onSubmit=\"\">\n                    <div className=\"form-group\">\n                        <label className=\"ratingLabel\" htmlFor=\"ratings\">Ratings:</label>\n                        <input className=\"border-0\" type=\"number\" placeholder=\"0\" max=\"5\" min=\"0\" onChange={data => setRatings(data.target.value)}/>\n                        <i className = \"fas fa-star text-muted\" />\n                    </div>\n                    <div className=\"form-group\">\n                        <label htmlFor=\"review\">Your Review:</label>\n                        <textarea className=\"form-control\" rows=\"5\" id=\"review\" onChange={data => setUserReview(data.target.value)} required></textarea>\n                        \n                    </div>\n                    {\n                        (user == null) ? (\n                            <p>Please <Link className=\"text-decoration-none text-important\" to=\"/login\">login</Link> to submit a review</p>\n                        ): \n                        (\n                            <div className=\"d-inline-flex\">\n                                {\n                                    submitted ? (\n                                        <button className=\"btn btn-success\" disabled>Submitted!</button>\n                                    ) : (\n                                            <button type =\"submit\" class=\"btn btn-light\" onClick={sendReview}>Submit</button>\n                                    )\n                                }\n                                \n                                <p className=\"m-0 align-items-center justify-content-end text-secondary p-3\">You are submitting as: <i>{user.username}</i></p>\n                            </div>\n                        )\n                        \n                }\n                </form>\n            </sec